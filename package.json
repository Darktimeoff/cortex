{
  "name": "express-v2",
  "version": "1.0.0",
  "description": "A Express v2 library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "oxlint --config ./oxlintrc.json --fix",
    "test": "jest --testMatch='**/!(tests)/**/*.test.ts' --passWithNoTests",
    "test:unit": "jest --testMatch='**/!(tests)/**/*.test.ts' --passWithNoTests",
    "test:integration": "jest --testMatch='**/tests/integration/**/*.test.ts' --passWithNoTests",
    "test:e2e": "jest --testMatch='**/tests/e2e/**/*.test.ts' --passWithNoTests",
    "test:all": "jest --passWithNoTests",
    "benchmark": "ts-node -r tsconfig-paths/register ./src/tests/performance.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "commit": "cz",
    "commitlint": "commitlint --edit",
    "dev": "nodemon",
    "start": "ts-node -r tsconfig-paths/register ./src/test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/autocannon": "^7.12.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.2",
    "autocannon": "^8.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "oxlint": "^0.16.1",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "files": [
    "dist/**/*"
  ],
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "memoirist": "^0.3.0"
  }
}
